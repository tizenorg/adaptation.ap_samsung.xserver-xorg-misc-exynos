[Unit]
Description=xserver
DefaultDependencies=no
Requires=tizen-generate-env.service
After=systemd-udev-trigger.service opt.mount smack-default-labeling.service tizen-generate-env.service

[Service]
Type=oneshot
RemainAfterExit=yes
Nice=-5
ExecStart=/usr/bin/startx
EnvironmentFile=-/run/tizen-mobile-env

[Install]
WantedBy=basic.target
